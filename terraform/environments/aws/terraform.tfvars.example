# Terraform Variables Configuration Example
# Copy this file to terraform.tfvars and update with your actual values

# Project Configuration
project_prefix      = "your-project-name"
environment         = "prod"  # or "staging", "dev"
aws_region          = "eu-west-1"  # or your preferred AWS region
kubernetes_version  = "1.33"  # Kubernetes version for EKS cluster

# VPC flow logs IAM role is now created automatically by the IAM module
# No manual IAM role creation required

# Optional: Custom tags for all resources
# Replace with your actual values
tags = {
  Owner       = "your-team@example.com"
  Environment = "prod"
  CostCenter  = "1234"
  Project     = "your-project-name"
  ManagedBy   = "terraform"
}
